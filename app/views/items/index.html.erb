<h2>To-Do Items</h2>
<div class="row">
<div class="col-md-8">
  <table class="table table-striped">
    <tbody>
      <% @items.each do |item| %>
        <tr>
          <td><%= item.name %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
<div class="col-md-4">
  <%= link_to "New", new_item_path, class: 'btn btn-primary' %>
</div>
</div>
